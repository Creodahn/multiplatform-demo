          "CompanyName": "NoneSuch",
          "FileDescription": "Multiplatform Demo",
          "OriginalFilename": "multiplatform-demo.exe",
          "ProductName": "Multiplatform Demo",
          "requested-execution-level": "asInvoker"
        }
      },
      "electronWinstallerConfig": {
        "authors": "justin.a.drew@gmail.com",
        "description": "Multiplatform Demo",
        "name": "Multiplatform.Demo",
        "setupExe": "Multiplatform Demo Installer.exe",
        "title": "Multiplatform Demo"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": ""
      }
